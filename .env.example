# Environment Configuration
RUNTIME_ENV=dev

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_DATABASE=opt_out_dev

# Service Ports
API_PORT=8000
UI_PORT=8501

# Database Management
WAIT_FOR_DB=false
MIGRATE_DB=true

# API Keys (Required for full functionality)
AGNO_API_KEY=your_agno_api_key_here
AGNO_MONITOR=your_agno_monitor_key_here
OPENAI_API_KEY=sk-your_openai_api_key_here
EXA_API_KEY=your_exa_api_key_here

# API Settings
API_TITLE="Agno Agents API"
API_VERSION="1.0.0"
API_DOCS_ENABLED=true
CORS_ORIGIN_LIST=http://localhost:8501,http://127.0.0.1:8501

# Debug Settings
PRINT_ENV_ON_LOAD=false